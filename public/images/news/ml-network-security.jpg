<svg width="800" height="400" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="aiGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#7c3aed;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#a855f7;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#ec4899;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="100%" height="100%" fill="url(#aiGrad)"/>
  
  <!-- Neural network pattern -->
  <g opacity="0.2">
    <defs>
      <pattern id="neural" width="60" height="60" patternUnits="userSpaceOnUse">
        <circle cx="30" cy="30" r="2" fill="white"/>
        <circle cx="10" cy="10" r="1.5" fill="white"/>
        <circle cx="50" cy="10" r="1.5" fill="white"/>
        <circle cx="10" cy="50" r="1.5" fill="white"/>
        <circle cx="50" cy="50" r="1.5" fill="white"/>
        <line x1="30" y1="30" x2="10" y2="10" stroke="white" stroke-width="0.5"/>
        <line x1="30" y1="30" x2="50" y2="10" stroke="white" stroke-width="0.5"/>
        <line x1="30" y1="30" x2="10" y2="50" stroke="white" stroke-width="0.5"/>
        <line x1="30" y1="30" x2="50" y2="50" stroke="white" stroke-width="0.5"/>
      </pattern>
    </defs>
    <rect width="100%" height="100%" fill="url(#neural)" />
  </g>
  
  <!-- AI Brain icon -->
  <g fill="rgba(255,255,255,0.9)" transform="translate(300, 80)">
    <!-- Brain outline -->
    <path d="M50 20 Q70 10 90 20 Q110 10 130 20 Q140 30 140 50 Q130 70 120 80 Q100 90 80 85 Q60 90 40 80 Q30 70 30 50 Q30 30 50 20 Z"/>
    <!-- Brain details -->
    <path d="M45 35 Q55 30 65 35 Q75 30 85 35" fill="none" stroke="rgba(255,255,255,0.7)" stroke-width="2"/>
    <path d="M45 45 Q55 40 65 45 Q75 40 85 45" fill="none" stroke="rgba(255,255,255,0.7)" stroke-width="2"/>
    <path d="M45 55 Q55 50 65 55 Q75 50 85 55" fill="none" stroke="rgba(255,255,255,0.7)" stroke-width="2"/>
    
    <!-- Neural connections -->
    <circle cx="40" cy="40" r="3" fill="rgba(255,255,255,0.8)"/>
    <circle cx="90" cy="35" r="3" fill="rgba(255,255,255,0.8)"/>
    <circle cx="70" cy="60" r="3" fill="rgba(255,255,255,0.8)"/>
    <line x1="40" y1="40" x2="70" y2="60" stroke="rgba(255,255,255,0.6)" stroke-width="1"/>
    <line x1="90" y1="35" x2="70" y2="60" stroke="rgba(255,255,255,0.6)" stroke-width="1"/>
  </g>
  
  <!-- Network security elements -->
  <g fill="rgba(255,255,255,0.8)">
    <!-- Server rack -->
    <rect x="100" y="250" width="40" height="60" rx="4" fill="rgba(255,255,255,0.9)"/>
    <rect x="105" y="255" width="30" height="8" fill="rgba(124,58,237,0.7)"/>
    <rect x="105" y="267" width="30" height="8" fill="rgba(124,58,237,0.7)"/>
    <rect x="105" y="279" width="30" height="8" fill="rgba(124,58,237,0.7)"/>
    <rect x="105" y="291" width="30" height="8" fill="rgba(124,58,237,0.7)"/>
    
    <!-- Data flow -->
    <rect x="200" y="280" width="300" height="2" fill="rgba(255,255,255,0.6)"/>
    <polygon points="500,276 520,281 500,286" fill="rgba(255,255,255,0.8)"/>
    
    <!-- Anomaly detection -->
    <circle cx="600" cy="280" r="20" fill="none" stroke="rgba(255,255,255,0.9)" stroke-width="2"/>
    <circle cx="600" cy="280" r="8" fill="rgba(255,59,48,0.8)"/>
    <text x="600" y="285" text-anchor="middle" font-family="Inter" font-size="10" fill="white">!</text>
  </g>
  
  <!-- Title -->
  <text x="400" y="200" text-anchor="middle" font-family="Inter, sans-serif" font-size="32" font-weight="bold" fill="white">
    Machine Learning
  </text>
  
  <!-- Subtitle -->
  <text x="400" y="230" text-anchor="middle" font-family="Inter, sans-serif" font-size="16" fill="rgba(255,255,255,0.8)">
    Network Anomaly Detection
  </text>
  
  <!-- Algorithm labels -->
  <g font-family="JetBrains Mono, monospace" font-size="12" fill="rgba(255,255,255,0.7)">
    <text x="50" y="380">Random Forest</text>
    <text x="250" y="380">Neural Networks</text>
    <text x="450" y="380">SVM</text>
    <text x="650" y="380">K-Means</text>
  </g>
</svg>
