                        <img src="/images/partners/telkom.svg" alt="Telkom University" loading="lazy">
                    </div>
                    <div class="partner-logo">
                        <img src="/images/partners/microsoft.svg" alt="Microsoft" loading="lazy">
                    </div>
                    <div class="partner-logo">
                        <img src="/images/partners/google.svg" alt="Google" loading="lazy">
                    </div>
                    <div class="partner-logo">
                        <img src="/images/partners/aws.svg" alt="Amazon Web Services" loading="lazy">
                    </div>
                    <div class="partner-logo">
                        <img src="/images/partners/nvidia.svg" alt="NVIDIA" loading="lazy">
                    </div>
