        <div class="container-custom">
            <div class="text-center mb-16" data-aos="fade-up">
                <h2 class="text-4xl md:text-5xl font-bold text-black mb-6">
                    Upcoming <span class="text-gradient">Events</span>
                </h2>
                <p class="text-xl text-black/70 max-w-3xl mx-auto">
                    Join our cybersecurity workshops, seminars, and conferences to enhance your skills
                </p>
            </div>
            
            <div class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-8 mb-12">
                <% upcomingEvents.slice(0, 3).forEach(function(event, index) { %>
                <article class="bg-white rounded-2xl shadow-xl hover:shadow-2xl transition-all duration-300 transform hover:-translate-y-2 overflow-hidden border border-gray-100" data-aos="fade-up" data-aos-delay="<%= (index + 1) * 100 %>">
                    <div class="relative overflow-hidden h-48">
                        <img src="<%= event.image %>" alt="<%= event.title %>" class="w-full h-full object-cover transition-transform duration-300 hover:scale-105">
                        <div class="absolute top-4 left-4">
                            <span class="bg-<%= event.category === 'workshop' ? 'blue' : event.category === 'seminar' ? 'green' : event.category === 'conference' ? 'purple' : 'yellow' %>-500 text-white px-3 py-1 rounded-full text-sm font-medium">
                                <%= event.category.charAt(0).toUpperCase() + event.category.slice(1) %>
                            </span>
                        </div>
                        <div class="absolute top-4 right-4">
                            <span class="bg-green-500 text-white px-2 py-1 rounded text-xs">
                                <%= event.status === 'upcoming' ? 'Upcoming' : event.status === 'ongoing' ? 'Ongoing' : 'Completed' %>
                            </span>
                        </div>
                        <div class="absolute bottom-4 right-4">
                            <span class="bg-black/50 text-white px-2 py-1 rounded text-xs">
                                <i class="fas fa-users mr-1"></i><%= event.spotsLeft %> spots left
                            </span>
                        </div>
                    </div>
                    
                    <div class="p-6">
                        <div class="flex items-center text-sm text-black/60 mb-3">
                            <span>
                                <i class="far fa-calendar-alt mr-1"></i>
                                <%= new Date(event.date).toLocaleDateString('id-ID', { day: 'numeric', month: 'long' }) %>
                            </span>
                            <span class="mx-2">•</span>
                            <span>
                                <i class="far fa-clock mr-1"></i>
                                <%= event.time %>
                            </span>
                        </div>
                        
                        <h3 class="text-xl font-bold text-black mb-3 line-clamp-2 hover:text-primary transition-colors">
                            <a href="/events/<%= event.slug %>"><%= event.title %></a>
                        </h3>
                        
                        <p class="text-black/70 mb-4 line-clamp-2">
                            <%= event.description %>
                        </p>
                        
                        <div class="flex items-center justify-between mb-4">
                            <div class="flex items-center text-sm text-black/60">
                                <i class="fas fa-map-marker-alt mr-1"></i>
                                <span><%= event.type === 'online' ? 'Online' : event.type === 'offline' ? event.location : 'Hybrid' %></span>
                            </div>
                            <div class="text-primary font-bold text-lg">
                                <% if (event.price === 0) { %>
                                    Free
                                <% } else { %>
                                    Rp <%= event.price.toLocaleString('id-ID') %>
                                <% } %>
                            </div>
                        </div>
                        
                        <div class="flex items-center justify-between">
                            <div class="flex flex-wrap gap-2">
                                <span class="bg-gray-100 text-gray-600 px-2 py-1 rounded-md text-xs">
                                    <%= event.type %>
                                </span>
                                <span class="bg-gray-100 text-gray-600 px-2 py-1 rounded-md text-xs">
                                    <%= event.level %>
                                </span>
                            </div>
                            
                            <a href="/events/<%= event.slug %>" class="text-primary hover:text-primary-600 font-medium text-sm flex items-center transition-colors">
                                View Details
                                <i class="fas fa-arrow-right ml-2 text-xs"></i>
                            </a>
                        </div>
                    </div>
                </article>
                <% }); %>
            </div>
            
            <div class="text-center">
                <a href="/events" class="btn btn-primary">
                    <i class="fas fa-calendar-alt mr-2"></i>
                    View All Events
                </a>
            </div>
        </div>
    </section>
    <% } %>

