    <!-- Latest News Section -->
    <% if (typeof featuredNews !== 'undefined' && featuredNews.length > 0) { %>
    <section class="section-padding bg-white">
        <div class="container-custom">
            <div class="text-center mb-16" data-aos="fade-up">
                <h2 class="text-4xl md:text-5xl font-bold text-black mb-6">
                    Latest <span class="text-gradient">News & Insights</span>
                </h2>
                <p class="text-xl text-black/70 max-w-3xl mx-auto">
                    Stay updated with the latest trends, technologies, and best practices in cybersecurity
                </p>
            </div>
            
            <div class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-8 mb-12">
                <% featuredNews.forEach(function(news, index) { %>
                <article class="bg-white rounded-2xl shadow-xl hover:shadow-2xl transition-all duration-300 transform hover:-translate-y-2 overflow-hidden border border-gray-100" data-aos="fade-up" data-aos-delay="<%= (index + 1) * 100 %>">
                    <div class="relative overflow-hidden h-48">
                        <img src="<%= news.imageUrl %>" alt="<%= news.title %>" class="w-full h-full object-cover transition-transform duration-300 hover:scale-105">
                        <div class="absolute top-4 left-4">
                            <span class="bg-primary text-white px-3 py-1 rounded-full text-sm font-medium">
                                <%= news.category %>
                            </span>
                        </div>
                        <div class="absolute top-4 right-4">
                            <span class="bg-black/50 text-white px-2 py-1 rounded text-xs">
                                <i class="far fa-clock mr-1"></i><%= news.readTime %>
                            </span>
                        </div>
                    </div>
                    
                    <div class="p-6">
                        <div class="flex items-center text-sm text-black/60 mb-3">
                            <span><%= new Date(news.publishDate).toLocaleDateString('id-ID', { year: 'numeric', month: 'long', day: 'numeric' }) %></span>
                            <span class="mx-2">•</span>
                            <span><%= news.author %></span>
                        </div>
                        
                        <h3 class="text-xl font-bold text-black mb-3 line-clamp-2 hover:text-primary transition-colors">
                            <a href="/news/<%= news.slug %>"><%= news.title %></a>
                        </h3>
                        
                        <p class="text-black/70 mb-4 line-clamp-3">
                            <%= news.excerpt %>
                        </p>
                        
                        <div class="flex items-center justify-between">
                            <div class="flex flex-wrap gap-2">
                                <% news.tags.slice(0, 2).forEach(function(tag) { %>
                                <span class="bg-gray-100 text-gray-600 px-2 py-1 rounded-md text-xs">
                                    #<%= tag %>
                                </span>
                                <% }); %>
                            </div>
                            
                            <a href="/news/<%= news.slug %>" class="text-primary hover:text-primary-600 font-medium text-sm flex items-center transition-colors">
                                Read More
                                <i class="fas fa-arrow-right ml-2 text-xs"></i>
                            </a>
                        </div>
                    </div>
                </article>
                <% }); %>
            </div>
            
            <div class="text-center">
                <a href="/news" class="btn btn-primary">
                    <i class="fas fa-newspaper mr-2"></i>
                    View All News
                </a>
            </div>
        </div>
    </section>
    <% } %>

    <!-- Upcoming Events Section -->
    <% if (typeof upcomingEvents !== 'undefined' && upcomingEvents.length > 0) { %>
