    <link href="https://unpkg.com/aos@2.3.1/dist/aos.css" rel="stylesheet">
</head>
<body class="bg-white home-page">
    <%- include('partials/navbar') %>

    <!-- Hero Section -->
    <section class="hero-section relative min-h-screen flex items-center justify-center overflow-hidden">
        <!-- Background -->
        <div class="absolute inset-0 gradient-bg"></div>
        <div class="absolute inset-0 bg-black/40"></div>
        
        <!-- Animated Background Elements -->
        <div class="absolute inset-0">
            <div class="absolute top-1/4 left-1/4 w-72 h-72 bg-cyber/20 rounded-full blur-3xl animate-pulse-slow"></div>
            <div class="absolute bottom-1/4 right-1/4 w-96 h-96 bg-primary/20 rounded-full blur-3xl animate-pulse-slow animation-delay-200"></div>
        </div>
        
        <!-- Content -->
        <div class="relative z-10 container-custom text-center">
            <div class="max-w-4xl mx-auto">
                <h1 class="text-5xl md:text-7xl font-bold text-white mb-6 animate-fade-in">
                    <span class="block">MBC</span>
                    <span class="text-mint">Laboratory</span>
                </h1>
                <h2 class="text-xl md:text-2xl text-cyber font-medium mb-8 animate-fade-in animation-delay-200">
                    Cybersecurity & Technology Solutions
                </h2>
                <p class="text-lg md:text-xl text-white mb-12 leading-relaxed max-w-3xl mx-auto animate-fade-in animation-delay-400">
                    Pusat Riset Teknologi dan Konsultan yang berfokus pada pengembangan solusi di bidang 
                    <span class="text-mint font-semibold">Cybersecurity</span>, 
                    <span class="text-cyber font-semibold">Big Data Analytics</span>, 
                    <span class="text-mint font-semibold">Game Technology</span>, dan 
                    <span class="text-cyber font-semibold">Geographic Information Systems (GIS)</span>.
                </p>
                <div class="flex flex-col sm:flex-row gap-6 justify-center animate-fade-in animation-delay-600">
                    <a href="/services" class="btn btn-cyber text-lg px-8 py-4">
                        <i class="fas fa-rocket mr-2"></i>Layanan Kami
                    </a>
                    <a href="/contact" class="btn btn-secondary text-lg px-8 py-4 bg-white/10 text-white border-white/30 hover:bg-white/20">
                        <i class="fas fa-phone mr-2"></i>Hubungi Kami
                    </a>
                </div>
            </div>
        </div>
        


        <!-- Scroll Indicator -->
        <div class="absolute bottom-8 left-1/2 transform -translate-x-1/2 animate-bounce">
            <a href="#about" class="text-white/70 hover:text-white">
                <i class="fas fa-chevron-down text-2xl"></i>
            </a>
        </div>
    </section>

    <!-- Partner Section -->
