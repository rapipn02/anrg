{
  "name": "anrg-laboratory",
  "version": "2.0.0",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "build-css": "tailwindcss -i ./src/input.css -o ./public/css/style.css",
    "watch-css": "tailwindcss -i ./src/input.css -o ./public/css/style.css --watch",
    "build": "npm run build-css",
    "deploy-vercel": "npm run build && vercel --prod",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "keywords": [
    "express",
    "tailwind",
    "cybersecurity",
    "artificial-intelligence",
    "network-research",
    "laboratory",
    "anrg"
  ],
  "author": "ANRG Laboratory",
  "license": "MIT",
  "description": "ANRG Laboratory Website - Advanced Network Research Group with Express and Tailwind CSS",
  "dependencies": {
    "axios": "^1.11.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "multer": "^2.0.2",
    "mysql2": "^3.14.2",
    "nodemailer": "^7.0.4",
    "validator": "^13.15.15"
  },
  "devDependencies": {
    "nodemon": "^3.1.10",
    "tailwindcss": "^3.4.17"
  }
}
